{
    "name": "asundust/dcat-wechat-work-push",
    "alias": "微信消息推送",
    "description": "利用企业微信应用消息向微信发送通知(无需公众号，不需要安装企业微信客户端，低成本推送消息解决方案)",
    "type": "library",
    "keywords": [
        "dcat-admin",
        "extension",
        "wechat",
        "work",
        "push"
    ],
    "homepage": "https://github.com/asundust/dcat-wechat-work-push",
    "license": "MIT",
    "authors": [
        {
            "name": "asundust",
            "email": "asundust@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "dcat/laravel-admin": "~2.0",
        "w7corp/easywechat": "4.*||5.*||>=6.0"
    },
    "autoload": {
        "psr-4": {
            "Asundust\\DcatWechatWorkPush\\": "src/"
        }
    },
    "extra": {
        "dcat-admin": "Asundust\\DcatWechatWorkPush\\DcatWechatWorkPushServiceProvider",
        "laravel": {
            "providers": [
                "Asundust\\DcatWechatWorkPush\\DcatWechatWorkPushServiceProvider"
            ]
        }
    }
}